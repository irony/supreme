{
  "name": "@iteam/supreme",
  "version": "2.13.1",
  "description": "A CLI tool that helps you get up an running fast",
  "author": {
    "name": "Rickard Laurin",
    "email": "rickard.laurin@gmail.com",
    "url": "https://willcodefor.beer"
  },
  "contributors": [
    {
      "name": "Jimmy Jardland"
    },
    {
      "name": "Andreas Lundqvist"
    }
  ],
  "repository": "github:Iteam1337/supreme",
  "homepage": "https://iteam1337.github.io/#/supreme",
  "bugs": "https://github.com/Iteam1337/supreme/issues",
  "main": "bin/supreme",
  "bin": "bin/supreme",
  "preferGlobal": true,
  "keywords": [
    "productivity",
    "cli",
    "generator"
  ],
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/dist",
    "/src"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "lint": "tsdx lint src test",
    "prepare": "npm run build",
    "test": "is-ci \"test:ci\" \"test:watch\"",
    "test:ci": "tsdx test",
    "test:watch": "tsdx test --watch",
    "test:coverage": "tsdx test --coverage"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@iteam/config": "12.1.1",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@types/app-root-path": "1.2.4",
    "@types/ejs": "3.0.5",
    "@types/inquirer": "6.5.0",
    "@types/jest": "25.2.3",
    "@types/meow": "5.0.0",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "husky": "4.3.0",
    "is-ci-cli": "2.1.2",
    "jest": "26.6.3",
    "jest-watch-typeahead": "0.6.1",
    "prettier": "2.1.2",
    "pretty-quick": "2.0.2",
    "tsdx": "0.14.1",
    "tslib": "1.14.1",
    "typescript": "3.9.7"
  },
  "dependencies": {
    "app-root-path": "3.0.0",
    "chalk": "4.1.0",
    "clipboardy": "2.3.0",
    "ejs": "3.1.5",
    "execa": "4.1.0",
    "inquirer": "7.3.2",
    "meow": "6.1.1",
    "ora": "4.1.1",
    "read-pkg-up": "7.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    }
  }
}
